{
    "compileOnSave": true,
    "compilerOptions": {
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "skipLibCheck": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "pretty": true
    }
}